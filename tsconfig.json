{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@components": [
        "./src/components"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@constants": [
        "./src/constants"
      ],
      "@containers/*": [
        "./src/containers/*"
      ],
      "@containers": [
        "./src/containers"
      ],
      "@common/*": [
        "./src/common/*"
      ],
      "@common": [
        "./src/common"
      ],
      "@server/*": [
        "./server/*"
      ],
      "@server": [
        "./server"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@services": [
        "./src/services"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@contexts": [
        "./src/contexts"
      ],
      "@hocs/*": [
        "./src/hocs/*"
      ],
      "@hocs": [
        "./src/hocs"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
