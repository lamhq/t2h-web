{
  "name": "t2hfe",
  "version": "1.0.0",
  "repository": "https://github.com/truck2hand/t2h-frontend",
  "author": "Taketo Yoshida <t_yoshida@tripetch-it.co.th>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development node server/index.js",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production node server/index.js",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx src/**/*",
    "format": "eslint -c .eslintrc.js --ext .ts,.tsx --fix src/**/*",
    "storybook": "start-storybook -p 6006 -c .storybook -s ./public",
    "build-storybook": "build-storybook",
    "preversion": "git-branch-is -r \"^(feature|fix|hotfix)/T2H-\\d+/[a-z\\-]+$\" && echo Preversion checks passed. Your branch name is correct format.",
    "analyze": "cross-env ANALYZE=true next build",
    "capture::server": "pageres .analyze/server.html --filename='.analyze/server' --hide='.Sidebar__container'",
    "capture::browser": "pageres .analyze/browser.html --filename='.analyze/browser' --hide='.Sidebar__container'",
    "export": "NODE_ENV=production next export"
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@date-io/moment": "1.3.13",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "3.2.10",
    "cookie-parser": "1.4.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "3.23.3",
    "http-proxy-middleware": "1.0.5",
    "isomorphic-unfetch": "3.0.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.27.0",
    "morgan": "1.10.0",
    "next": "9.5.0",
    "next-cookies": "2.0.3",
    "next-i18next": "4.5.0",
    "next-offline": "5.0.2",
    "nprogress": "0.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-google-recaptcha": "2.1.0",
    "react-hook-form": "5.7.2",
    "react-lazyload": "3.0.0",
    "react-sortablejs": "2.0.11",
    "styled-components": "5.1.1",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.1",
